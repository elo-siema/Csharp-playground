@using ControllerTest2.ViewModels
@model FileUploadViewModel
@{
    Layout = "~/Views/Shared/MyLayout.cshtml";
}

@section TitleSection{
    Bulk Upload
    }

@section HeaderSection{
    <script src="~/Scripts/FileValidation.js"></script>
    }

@section ContentBody{
    <div>
        <a href="/Employee/Index">Back</a>
        <form action="/BulkUpload/Upload" method="post" enctype="multipart/form-data">
            Select file : <input type="file" name="fileUpload" value="" onchange="ValidateFile(this)" />
            <input type="submit" name="name" value="upload" />
        </form>
    </div>
    }